  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "prebuild:clear": "sudo rm -rf ./android && sudo rm -rf ./ios",
    "android:client": "sudo rm -rf android && sudo rm -rf ios && NODE_ENV=production sudo npx expo prebuild --clean -p android && sudo chmod -R 777 android && cd android && ./gradlew clean && cd .. && NODE_ENV=production sudo eas build --profile local_client_build --platform android --local",
    "android:sim": "sudo rm -rf android && sudo rm -rf ios && NODE_ENV=test sudo npx expo prebuild --clean -p android && sudo chmod -R 777 android && cd android && ./gradlew clean && cd .. && NODE_ENV=test sudo eas build --profile local_android_simulator --platform android --local",
    "ios:dev": "sudo rm -rf android && sudo rm -rf ios && sudo npx expo prebuild --clean -p ios && sudo chmod -R 777 ios  && cd ios && sudo pod install --allow-root && cd .. && sudo eas build --profile ios-development --platform ios --local",
    "ios:sim": "sudo rm -rf ./android && sudo rm -rf ./ios && NODE_ENV=test sudo npx expo prebuild --clean -p ios && sudo chmod -R 777 ios && cd ios && sudo pod install --allow-root && cd .. && sudo eas build --profile ios-simulator --platform ios --local"
  },
